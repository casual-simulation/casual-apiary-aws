{
    "name": "casual-apiary-aws",
    "version": "1.0.0",
    "private": true,
    "description": "Serverless CasualOS on AWS",
    "main": "handler.js",
    "scripts": {
        "start": "serverless offline start --stage dev",
        "test:watch": "jest --watchAll",
        "test": "jest --verbose",
        "test:ci": "jest --no-cache --ci --reporters=default",
        "dynamodb:install": "serverless dynamodb install --stage dev",
        "deploy:dev": "serverless deploy --stage dev",
        "deploy:casualos-dev": "serverless deploy --stage casualos-dev",
        "deploy:boormanlabs": "serverless deploy --stage boormanlabs",
        "deploy:casualos": "serverless deploy --stage casualos-redis",
        "deploy:publicos": "serverless deploy --stage publicos-prod",
        "deploy:auxbackend": "serverless deploy --stage auxbackend-prod"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "dependencies": {
        "@casual-simulation/aux-common": "^3.0.4",
        "@casual-simulation/aux-vm": "^3.0.4",
        "@casual-simulation/causal-trees": "^3.0.2",
        "yjs": "13.5.24",
        "aws-sdk": "2.789.0",
        "axios": "0.21.0",
        "base64-js": "^1.5.1",
        "fast-json-stable-stringify": "2.1.0",
        "hash.js": "1.1.7",
        "lodash": "4.17.20",
        "rxjs": "6.6.3",
        "scrypt-js": "3.0.1",
        "source-map-support": "^0.5.10",
        "tweetnacl": "1.0.3",
        "uuid": "^8.3.2",
        "redis": "3.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.12.3",
        "@babel/preset-env": "^7.12.1",
        "@types/aws-lambda": "^8.10.17",
        "@types/base64-js": "^1.3.0",
        "@types/jest": "^26.0.16",
        "@types/node": "^10.12.18",
        "@types/serverless": "^1.72.5",
        "@types/redis": "2.8.28",
        "babel-jest": "^26.6.3",
        "fork-ts-checker-webpack-plugin": "^3.0.1",
        "husky": "^4.3.0",
        "jest": "^26.6.3",
        "jest-environment-node": "^26.6.2",
        "prettier": "^2.1.2",
        "pretty-quick": "^3.0.2",
        "serverless-dynamodb-local": "0.2.39",
        "serverless-offline": "6.8.0",
        "serverless-s3-local": "0.6.7",
        "serverless-webpack": "5.2.0",
        "ts-jest": "^26.4.4",
        "ts-loader": "^8.0.11",
        "ts-node": "^9.1.0",
        "typescript": "^4.1.2",
        "webpack": "^4.29.0",
        "webpack-node-externals": "^1.7.2",
        "serverless": "2.11.1"
    },
    "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
    "license": "MIT"
}
